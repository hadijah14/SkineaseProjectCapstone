<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.skineast">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SkinEast"
        tools:targetApi="31">
        <activity
            android:name=".halaman_penyakit.jerawat"
            android:exported="false" />
        <activity
            android:name=".halaman_penyakit.kemerahan"
            android:exported="false" />
        <activity
            android:name=".halaman_penyakit.kusta"
            android:exported="false" />
        <activity
            android:name=".halaman_penyakit.shingles"
            android:exported="false" />
        <activity
            android:name=".halaman_penyakit.kurap"
            android:exported="false" />
        <activity
            android:name=".halaman_penyakit.kutil"
            android:exported="false" />
        <activity
            android:name=".halanan_list"
            android:exported="false" />
        <activity
            android:name=".merawat_tropis"
            android:exported="false" />
        <activity
            android:name=".HalamanDetail"
            android:exported="false" />
        <activity
            android:name=".HalamanKamera"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.dicoding.picodiploma.mycamera"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>

</manifest>